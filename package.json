{
  "name": "Rhamseys Portfolio",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "_comment": "npm install --save-dev concurrently",
  "scripts": {
    "deploy": "npm run build && mv dist ./dist_version_control/dist_$(date +%Y%m%d%H%M%S)",
    "dev": "concurrently \"npm run server:dev\" \"npm run server:client\"",
    "start": "node backEnd/server.js",
    "prod": "npm run lint && npm run build && npm run server:prod",
    "server": "nodemon backend/server.js",
    "client": "npm start --prefix client",
    "server:dev": "nodemon backEnd/server.js",
    "server:client": "npm start --prefix client",
    "server:prod": "cross-env NODE_ENV=production node backEnd/server.js",
    "data:import": "node backend/seeder.js",
    "data:destroy": "node backend/seeder.js -d",
    "build": "npm install && npm install --prefix client && npm run build --prefix client && mkdir -p dist_version_control && cp -r client/build dist_version_control/dist_$(date +%Y%m%d_%H%M%S)",
    "lint-old": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "lint": "eslint . --max-warnings 0",
    "preview": "vite preview"
  },
  "dependencies": {
    "@emailjs/browser": "^3.11.0",
    "@react-three/drei": "^9.93.0",
    "@react-three/fiber": "^8.15.13",
    "@vitejs/plugin-react": "^4.2.1",
    "axios": "^1.6.5",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cross-env": "^7.0.3",
    "dotenv": "^16.3.1",
    "eslint": "^8.55.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "express": "^4.19.2",
    "framer-motion": "^10.17.9",
    "jsonwebtoken": "^9.0.2",
    "maath": "^0.10.7",
    "mongoose": "^8.4.3",
    "multer": "^1.4.5-lts.1",
    "node-fetch": "^3.3.2",
    "prop-types": "15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.21.1",
    "react-vertical-timeline-component": "^3.6.0",
    "sirv": "^2.0.3",
    "vike": "^0.4.176"
  },
  "devDependencies": {
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.16",
    "concurrently": "^8.2.2",
    "eslint": "^8.55.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "postcss": "^8.4.33",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-tilt": "^1.0.2",
    "tailwindcss": "^3.4.4",
    "vike": "^0.4.171",
    "vite": "^5.3.1"
  }
}
